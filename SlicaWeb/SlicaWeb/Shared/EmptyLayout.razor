@inherits LayoutComponentBase
<MudThemeProvider Theme="MyCustomTheme" />
<MudSnackbarProvider />
<MudDialogProvider />

<MudLayout>

  
    <MudMainContent>
        @Body
    </MudMainContent>
</MudLayout>

@code {
    MudTheme MyCustomTheme = new MudTheme()
        {
            Palette = new PaletteLight()
            {
                Primary = Colors.Shades.Black,
                
            },
            PaletteDark = new PaletteDark()
            {
                Primary = Colors.Shades.Black
            },

        };
}
